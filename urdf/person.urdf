<robot name="person">
  <material name="red">
    <color rgba="0 0 0.8 1"/>
  </material>
  <link name="base_footprint">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
  </link>
  <link name="body">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.236" length="1.414"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.236" length="1.414"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="48.359"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="8.057" ixy="0.0" ixz="0.0" iyy="8.057" iyz="0.0" izz="1.347"/>
    </inertial>
  </link>
  <joint name="joint_body" type="fixed">
    <parent link="base_footprint"/>
    <child link="body"/>
    <origin xyz="0 0 0.707" rpy="0 0 0"/>
  </joint>
  <link name="head">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.118" length="0.236"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.118" length="0.236"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="8.071"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <joint name="joint_head" type="fixed">
    <parent link="base_footprint"/>
    <child link="head"/>
    <origin xyz="0 0 1.532" rpy="0 0 0"/>
  </joint>
  <gazebo>
    <plugin name="object_controller" filename="libgazebo_ros_planar_move.so">
      <commandTopic>vel_person3</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <odometryRate>20.0</odometryRate>
      <robotBaseFrame>base_footprint</robotBaseFrame>
    </plugin>
  </gazebo>
</robot>
